[
  {
    "name": "Markdown Headings",
    "scope": "markup.heading.markdown",
    "settings": {
      "foreground": "{{palette.blue.sweet}}"
    }
  },
  {
    "name": "Markdown Italics",
    "scope": "markup.italic.markdown",
    "settings": {
      "foreground": "{{palette.purple}}",
      "fontStyle": "italic"
    }
  },
  {
    "name": "Markdown Bold",
    "scope": "markup.bold.markdown",
    "settings": {
      "foreground": "{{palette.yellow.dark}}",
      "fontStyle": "bold"
    }
  },
  {
    "name": "Markdown Quote + others",
    "scope": "markup.quote.markdown",
    "settings": {
      "foreground": "{{palette.grey.mild}}",
      "fontStyle": "italic"
    }
  },
  {
    "name": "Markdown Raw Code + others",
    "scope": "markup.inline.raw.markdown",
    "settings": {
      "foreground": "{{palette.green.cool}}"
    }
  },
  {
    "name": "Markdown Links",
    "scope": [
      "markup.underline.link.markdown",
      "markup.underline.link.image.markdown"
    ],
    "settings": {
      "foreground": "{{palette.red.cool}}"
    }
  },
  {
    "name": "Markdown Link Title and Description",
    "scope": [
      "string.other.link.title.markdown",
      "string.other.link.description.markdown"
    ],
    "settings": {
      "foreground": "{{palette.grey.grey}}"
    }
  },
  {
    "name": "Markdown Punctuation",
    "scope": [
      "punctuation.definition.string.markdown",
      "punctuation.definition.string.begin.markdown",
      "punctuation.definition.string.end.markdown",
      "meta.link.inline.markdown punctuation.definition.string"
    ],
    "settings": {
      "foreground": "{{palette.blue.sweet}}"
    }
  },
  {
    "name": "Markdown MetaData Punctuation",
    "scope": [
      "punctuation.definition.metadata.markdown"
    ],
    "settings": {
      "foreground": "{{palette.red.light}}"
    }
  },
  {
    "name": "Markdown List Punctuation",
    "scope": [
      "beginning.punctuation.definition.list.markdown"
    ],
    "settings": {
      "foreground": "{{palette.blue.sea}}"
    }
  }
]
