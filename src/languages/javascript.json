      "foreground": "{{palette.grey.grey}}"
    }
  },
  {
    "name": "JavaScript Punctuation Definition",
    "scope": "",
    "settings": {
      "foreground": "{{palette.grey.grey}}"
    }
  },
  {
    "name": "JavaScript Meta Punctuation Definition",
    "scope": "meta.js punctuation.definition.js",
    "settings": {
      "foreground": "{{palette.grey.grey}}"
    }
